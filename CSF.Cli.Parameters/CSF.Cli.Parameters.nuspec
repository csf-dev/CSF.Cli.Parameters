    <copyright>Copyright 2016</copyright>
    <dependencies>
      <dependency id="CSF.Utils" version="[6.0.0,7.0.0)" />
      <dependency id="CSF.Reflection" version="[1.0.0,2.0.0)" />
    </dependencies>
  </metadata>
  <files>
    <file src="bin/Release/CSF.Cli.Parameters.dll" target="lib/net45" />
    <file src="bin/Release/CSF.Cli.Parameters.xml" target="lib/net45" />
  </files>
</package>
